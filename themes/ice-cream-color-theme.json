    "tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#6D6F7C",
                "fontStyle": "italic"
			}
		},
		{
			"name": "Keywords",
			"scope": ["keyword", "storage.type.struct", "storage.modifier.static", "storage.modifier.inline", "storage.modifier.c", "storage.modifier.specifier", "storage.type.built-in"],
			"settings": {
				"foreground": "#F0A8FA"
			}
		},
		{
			"name": "Literals",
			"scope": ["constant", "keyword.other.unit", "string"],
			"settings": {
				"foreground": "#dfffbc"
			}
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#F0A8FA"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type",
				"entity.name.type.parameter",
				"entity.name.class",
				"storage.type.generic",
				"storage.type.primitive",
	        ],
			"settings": {
				"foreground": "#B294F7"
			}
		},
		{
			"name": "Functions",
			"scope": ["entity.name.function", "entity.name.methods"],
			"settings": {
				"foreground": "#FFF1E3"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable"],
			"settings": {
				"foreground": "#F5D0FA",
				"fontStyle": ""
			}
		},
        {
			"name": "Param Variables",
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#A7A8AF",
				"fontStyle": ""
			}
		},
		{
			"name": "Constants",
			"scope": ["variable.other.enummember", "variable.other.constant"],
			"settings": {
				"foreground": "#dfffbc",
				"fontStyle": ""
			}
		},
		{
			"name": "Macros",
			"scope": ["entity.name.function.preprocessor"],
			"settings": {
				"foreground": "#FFDEBF",
				"fontStyle": ""
			}
		},
		{
			"name": "Parameters",
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#A7A8AF",
				"fontStyle": ""
			}
		},
		{
			"name": "Source",
			"scope": ["source"],
			"settings": {
				"foreground": "#A7A8AF",
				"fontStyle": ""
			}
		},
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.escape",
                "keyword.other.unit",
                "keyword.other",
                "punctuation.separator.key-value.html"
            ],
            "settings": {
                "foreground": "#dfffbc"
            }
        },
        {
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "markup.changed.git_gutter",
                "support.type.sys-types",
            ],
            "settings": {
                "foreground": "#A7A8AF"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.id",
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.pseudo-class",
                "entity.name.tag.css",
                "keyword.control.conditional.js",
                "support.constant",
                "entity.other.attribute-name.html",
                "string.quoted.single.js",
                "storage.type.function.js",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#FFDEBF"
            }
        },
        {
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#F5D0FA"
            }
        },
        {
            "scope": [
                "*url*",
                "*link*",
                "*uri*"
            ],
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "meta.separator"
            ],
            "settings": {
                "foreground": "#A7A8AF",
                "fontStyle": ""
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#B294F7"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#dfffbc"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#FF293B"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#F5D0FA"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "macro": "#FBCD83",
        "variable": "#FFF1E3",
        "type": "#B294F7"
    }
}