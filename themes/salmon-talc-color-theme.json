	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#846872e3" 
			}
		},
		{
			"name": "Keywords",
			"scope": ["keyword", "storage.type.struct", "storage.modifier.static", "storage.modifier.inline", "storage.modifier.c", "storage.modifier.specifier", "storage.type.built-in"],
			"settings": {
				"foreground": "#e097b2"
			}
		},
		{
			"name": "Literals",
			"scope": ["constant", "keyword.other.unit", "string"],
			"settings": {
				"foreground": "#92c9bc"
			}
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#e097b2"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"storage.type.generic",
				"storage.type.primitive",
				"meta.function.definition.parameters"],
			"settings": {
				"foreground": "#ffcbbf"
			}
		},
		{
			"name": "Functions",
			"scope": ["entity.name.function", "entity.name.methods"],
			"settings": {
				"foreground": "#d5d5d5"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable"],
			"settings": {
				"foreground": "#d9d9d9",
				"fontStyle": ""
			}
		},
		{
			"name": "Constants",
			"scope": ["variable.other.enummember", "variable.other.constant"],
			"settings": {
				"foreground": "#92c9bc",
				"fontStyle": ""
			}
		},
		{
			"name": "Macros",
			"scope": ["entity.name.function.preprocessor"],
			"settings": {
				"foreground": "#92c9bc",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Parameters",
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#d9d9d9",
				"fontStyle": ""
			}
		},
		{
			"name": "Source",
			"scope": ["source"],
			"settings": {
				"foreground": "#9c9c9c",
				"fontStyle": ""
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"type": "#ffcbbf",
		"namespace": "#e097b2",
		"keyword": "#e097b2",
		"static": "#e097b2",
		"field": "#e097b2",
	}
